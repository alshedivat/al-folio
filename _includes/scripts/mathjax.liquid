{% if site.enable_math %}
  {% unless page.pseudocode %}
    <!-- MathJax -->
    <script type="text/javascript">
      window.MathJax = {
        tex: {
          tags: 'ams',
        },
      };
    </script>
    <script
      defer
      type="text/javascript"
      id="MathJax-script"
      src="{{ site.third_party_libraries.mathjax.url.js }}"
    ></script>
    <script defer src="{{ site.third_party_libraries.polyfill.url.js }}"></script>
  {% endunless %}
{% endif %}
