@article{anderson.etal_2013,
  title = {Rock Damage and Regolith Transport by Frost: An Example of Climate Modulation of the Geomorphology of the Critical Zone},
  shorttitle = {Rock Damage and Regolith Transport by Frost},
  author = {Anderson, Robert S. and Anderson, Suzanne P. and Tucker, Gregory E.},
  date = {2013-03-15},
  journaltitle = {Earth Surf. Process. Landforms},
  volume = {38},
