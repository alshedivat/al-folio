@article{rabatel.etal_2012,
  title = {Can the Snowline Be Used as an Indicator of the Equilibrium Line and Mass Balance for Glaciers in the Outer Tropics?},
  author = {Rabatel, Antoine and Bermejo, Ana and Loarte, Edwin and Soruco, Alvaro and Gomez, Jesus and Leonardini, Gonzalo and Vincent, Christian and Sicart, Jean Emmanuel},
  date = {2012},
