@article{doi:10.1126/sciadv.ade7692,
  abstract = {Preterm birth (PTB) is the leading cause of death in children under five, yet comprehensive studies are hindered by its multiple complex etiologies. Epidemiological associations between PTB and maternal characteristics have been previously described. This work used multiomic profiling and multivariate modeling to investigate the biological signatures of these characteristics. Maternal covariates were collected during pregnancy from 13,841 pregnant women across five sites. Plasma samples from 231 participants were analyzed to generate proteomic, metabolomic, and lipidomic datasets. Machine learning models showed robust performance for the prediction of PTB (AUROC = 0.70), time-to-delivery (r = 0.65), maternal age (r = 0.59), gravidity (r = 0.56), and BMI (r = 0.81). Time-to-delivery biological correlates included fetal-associated proteins (e.g., ALPP, AFP, and PGF) and immune proteins (e.g., PD-L1, CCL28, and LIFR). Maternal age negatively correlated with collagen COL9A1, gravidity with endothelial NOS and inflammatory chemokine CXCL13, and BMI with leptin and structural protein FABP4. These results provide an integrated view of epidemiological factors associated with PTB and identify biological signatures of clinical covariates affecting this disease. Deep biological profiling and machine learning reveal insights into the epidemiology of preterm birth.},
  added-at = {2023-05-29T17:20:46.000+0200},
  author = {Espinosa, Camilo A. and Khan, Waqasuddin and Khanam, Rasheda and Das, Sayan and Khalid, Javairia and Pervin, Jesmin and Kasaro, Margaret P. and Contrepois, Kévin and Chang, Alan L. and Phongpreecha, Thanaphong and Michael, Basil and Ellenberger, Mathew and Mehmood, Usma and Hotwani, Aneeta and Nizar, Ambreen and Kabir, Furqan and Wong, Ronald J. and Becker, Martin and Berson, Eloise and Culos, Anthony and Francesco, Davide De and Mataraso, Samson and Ravindra, Neal and Thuraiappah, Melan and Xenochristou, Maria and Stelzer, Ina A. and Marić, Ivana and Dutta, Arup and Raqib, Rubhana and Ahmed, Salahuddin and Rahman, Sayedur and Hasan, A. S. M. Tarik and Ali, Said M. and Juma, Mohamed H. and Rahman, Monjur and Aktar, Shaki and Deb, Saikat and Price, Joan T. and Wise, Paul H. and Winn, Virginia D. and Druzin, Maurice L. and Gibbs, Ronald S. and Darmstadt, Gary L. and Murray, Jeffrey C. and Stringer, Jeffrey S. A. and Gaudilliere, Brice and Snyder, Michael P. and Angst, Martin S. and Rahman, Anisur and Baqui, Abdullah H. and Jehan, Fyezah and Nisar, Muhammad Imran and Vwalika, Bellington and Sazawal, Sunil and Shaw, Gary M. and Stevenson, David K. and Aghaeepour, Nima},
